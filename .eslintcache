e={filterBy === 'all'}\n                onClick={handleItemClick}\n            >Все</Menu.Item>\n\n            <Menu.Item\n                name='price_high'\n                active={filterBy === 'price_high'}\n                onClick={handleItemClick}\n            >Сначала дорогие</Menu.Item>\n\n            <Menu.Item\n                name='price_low'\n                active={filterBy === 'price_low'}\n                onClick={handleItemClick}\n            >Сначала дешёвые</Menu.Item>\n\n            <Menu.Item\n                name='author'\n                active={filterBy === 'author'}\n                onClick={handleItemClick}\n            >Автор</Menu.Item>\n\n            <Input value={searchQuery} onChange={(e)=>{setQuery(e.target.value)}} icon={'search'} placeholder={'Введите название'}></Input>\n\n        </Menu>\n    )\n}\n\nexport default Filter;","/Users/almazsaripov/Documents/Projects/react-shopping/src/actions/filter.js",[],"/Users/almazsaripov/Documents/Projects/react-shopping/src/reducers/filter.js",["79"],"const initialState={\n    filterBy: '',\n    searchQuery: ''\n}\n\nexport default (state=initialState,action)=>{\n    switch (action.type)\n    {\n        case 'SET_FILTER': {\n        return {\n            ...state,\n            filterBy: action.payload\n        }\n    }\n        case 'SET_QUERY': {\n            return {\n                ...state,\n                searchQuery: action.payload\n            }\n        }\n        default:\n            return state;\n    }\n\n}\n","/Users/almazsaripov/Documents/Projects/react-shopping/src/actions/cart.js",[],"/Users/almazsaripov/Documents/Projects/react-shopping/src/container/BookCard.js",["80","81","82","83","84","85","86","87","88"],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":5,"column":1,"nodeType":"95","endLine":8,"endColumn":2},{"ruleId":"93","severity":1,"message":"94","line":6,"column":1,"nodeType":"95","endLine":28,"endCol